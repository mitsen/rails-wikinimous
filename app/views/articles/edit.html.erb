<h1>Articles#edit</h1>
<p>Find me in app/views/articles/edit.html.erb</p>

<h1>Edit this Article</h1>
<%= form_for @article do |f|%>
  <%= f.label :title, "Enter a title" %>
  <%= f.text_field :title %>

  <%= f.label :content, "Enter the content" %>
  <%= f.text_field :content %>

  <%= f.submit %>
<% end %>
