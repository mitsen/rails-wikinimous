<h1>Articles#new</h1>
<p>Find me in app/views/articles/new.html.erb</p>

<h1>New Article</h1>
<%= form_for @article do |f|%>
  <%= f.label :title, "Enter a title" %>
  <%= f.text_field :title %>

  <%= f.label :content, "Enter the content" %>
  <%= f.text_area :content %>

  <%= f.submit %>
<% end %>
